(:import tuple)

%-

; TODO tydecl sex = Boy | Girl

; see https://github.com/vmchale/doggo-command-line/blob/master/src/main.rs
(:def greeter
  (:lambda dog (text, text)
    (:let
      [name ($fst dog)]
      [pronoun ($snd dog)]
        (:oneof
          (| "${name} eats toilet paper sometimes but ${pronoun} tries.")))))

(:def main
  ($ greeter ("Maxine", "she")))
